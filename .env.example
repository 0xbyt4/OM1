# OM1 Environment Configuration
# Copy this file to .env and fill in your values
#
# Quick setup: cp .env.example .env
# Then edit .env with your API keys

# ===================
# REQUIRED (for most configs)
# ===================
OM_API_KEY=your_openmind_api_key_here

# ===================
# LLM PROVIDERS (choose based on your config)
# ===================

# OpenAI (for open_ai.json5, conversation.json5)
# Get key: https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-...

# Google Gemini (for gemini.json5)
# Get key: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=

# DeepSeek (for deepseek.json5)
# Get key: https://platform.deepseek.com
DEEPSEEK_API_KEY=

# xAI Grok (for grok.json5)
# Get key: https://console.x.ai
XAI_API_KEY=

# OpenRouter (for openrouter.json5)
# Get key: https://openrouter.ai/keys
OPENROUTER_API_KEY=

# Ollama (for ollama.json5)
# No API key needed! Just run: ollama serve
# Then: ollama pull llama3.2

# ===================
# SPEECH SERVICES
# ===================

# ElevenLabs TTS (for text-to-speech output)
# Get key: https://elevenlabs.io
ELEVENLABS_API_KEY=

# ===================
# INTEGRATIONS
# ===================

# Twitter/X (for twitter.json5)
# Get keys: https://developer.twitter.com
TWITTER_API_KEY=
TWITTER_API_SECRET=
TWITTER_ACCESS_TOKEN=
TWITTER_ACCESS_SECRET=

# DIMO/Tesla (for tesla.json5)
# Get credentials: https://dimo.zone
DIMO_CLIENT_ID=
DIMO_API_KEY=

# Coinbase CDP (for wallet configs)
# Get key: https://portal.cdp.coinbase.com
CDP_API_KEY=
COINBASE_WALLET_ID=
COINBASE_API_KEY=
COINBASE_API_SECRET=

# Ethereum wallet integration
ETH_ADDRESS=

# ===================
# ROBOTICS
# ===================

# Robot IP address (for physical robot connections)
ROBOT_IP=192.168.0.241

# Unique Robot ID (for multi-robot setups)
URID=default

# ===================
# OPTIONAL
# ===================

# ROS Domain ID (for ROS2 integration)
ROS_DOMAIN_ID=0
